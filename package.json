{
  "name": "queueoverflowsuper",
  "version": "1.0.0",
  "description": "Queueoverflow app",
  "scripts": {
    "build:app": "docker-compose up --build",
    "start:app": "docker-compose up",
    "start:elastic": "docker run --name queueoverflow_elasticsearch --net queueoverflow -p 9200:9200 -p 9300:9300 -it elasticsearch:8.10.2",
    "down:app": "docker-compose down",
    "release:shared": "cd libs/shared && yarn install && yarn build && npm version patch && yarn publish:package",
    "update:package": "node ./scripts/update-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hooannn/QueueOverflow.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hooannn/QueueOverflow/issues"
  },
  "homepage": "https://github.com/Hooannn/QueueOverflow#readme",
  "devDependencies": {
    "commander": "^11.0.0",
    "inquirer": "^8.0.0"
  }
}
